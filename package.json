{
  "name": "pwa-chat",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:prod": "http-server ./dist",
    "dist": "npm run build && npm run serve:prod",
    "build": "npm run build:sw && vue-cli-service build",
    "build:sw": "vue-cli-service build --target lib --name sw --dest dist-sw/ ./src/sw/sw.js && cp dist-sw/sw.umd.min.js dist-sw/sw.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "firebase-admin": "^10.0.0",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "eruda": "^2.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "firebase": "^9.3.0",
    "http-server": "^14.0.0",
    "https-server": "^0.1.2",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0"
  }
}
